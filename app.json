{
  "expo": {
    "name": "Aveno",
    "slug": "aveno",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "plugins": [
      "expo-notifications",
      [
        "expo-sqlite",
        {
          "enableFTS": true
        }
      ]
    ],
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/dark/hookflow_dark_ios_1024x1024.png",
      "bundleIdentifier": "com.aveno.app"
    },
    "android": {
      "icon": "./assets/dark/hookflow_dark_android_432x432.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "package": "com.aveno.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "icon": "./assets/dark/hookflow_dark_web_512x512.png",
      "bundler": "metro",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "eb6984a1-e197-44b9-b038-950f9b23b1a7"
      }
    }
  }
}
